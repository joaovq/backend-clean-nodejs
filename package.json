{
  "name": "clean-arch",
  "version": "1.0.0",
  "description": "Clean arch node template",
  "author": "JoÃ£o Vitor Q Mattos",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/joaovq/backend-clean-nodejs",
  "scripts": {
    "preitest": "yarn migrates",
    "test": "yarn unit-test && yarn integration-test",
    "preintegration-test": "yarn migrates",
    "migrates": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test npx sequelize-cli db:seed:all",
    "integration-test": "NODE_ENV=test jest --detectOpenHandles src/test/integration",
    "jest": "jest --detectOpenHandles --runTestsByPath",
    "coverage": "jest --detectOpenHandles --coverage",
    "postintegration-test": "yarn undo-migrates",
    "undo-migrates": "NODE_ENV=test sequelize db:migrate:undo:all",
    "preone-integration-test": "yarn migrates",
    "one-integration-test": "NODE_ENV=test yarn jest",
    "postone-integration-test": "yarn undo-migrates",
    "one-unit-test": "NODE_ENV=test yarn jest",
    "unit-test": "NODE_ENV=test yarn coverage jest src/test/unit",
    "lint": "./node_modules/.bin/eslint --fix src/",
    "verify-dependencies-rules": "npx depcruise --config .dependency-cruiser.js src",
    "code-analyses": "sonarqube-verify",
    "audit-dependencies": "audit-ci --config audit-ci.json",
    "set-up-database": "yarn sequelize db:migrate",
    "create-database": "yarn clean-database && yarn set-up-database && yarn sequelize-cli db:seed:all",
    "clean-database": "yarn sequelize-cli db:seed:undo:all",
    "create-database-diagram": "yarn script src/infra/database/scripts/erd.ts",
    "create-uml-diagram": "tplant --input src/**/*.ts --output ./uml-diagram.svg",
    "build": "rimraf dist && tsc",
    "start": "node -r module-alias/register dist/main/server.js",
    "production": "NODE_ENV=production yarn start",
    "dev": "NODE_ENV=dev yarn build-docker && ts-node-dev -r tsconfig-paths/register --transpile-only src/main/server.ts",
    "build-docker": "docker-compose up -d",
    "script": "ts-node -r tsconfig-paths/register",
    "create-dependency-graph": "npx depcruise --include-only \"^src\" --output-type dot src | dot -T svg > docs/dependencygraph.svg",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.14",
    "@types/chai-datetime": "^0.0.36",
    "@types/express": "^4.17.9",
    "@types/factory-girl": "^5.0.3",
    "@types/faker": "^5.1.5",
    "@types/form-data": "^2.5.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.162",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.10",
    "@types/readline-sync": "^1.4.3",
    "@types/signale": "^1.4.1",
    "@types/sinon": "^9.0.10",
    "@types/string-similarity": "^3.0.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "audit-ci": "^3.1.1",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.7.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "dependency-cruiser": "^9.22.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "factory-girl": "^5.0.4",
    "faker": "^5.2.0",
    "faker-br": "^0.3.0",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "sequelize-erd": "^1.3.1",
    "sinon": "^9.2.2",
    "sonarqube-scanner": "^2.8.0",
    "sonarqube-verify": "^1.0.2",
    "sqlite3": "^5.0.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "aws-sdk": "^2.778.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "graphql": "^15.4.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.4",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "readline-sync": "^1.4.10",
    "sequelize": "^6.3.5",
    "sequelize-json-schema": "^2.1.1",
    "signale": "^1.4.0",
    "string-similarity": "^4.0.2"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
